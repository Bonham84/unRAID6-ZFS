<FILE Name="&plugin;/packages/spl-0.6.5.1-unRAID-6.1.3.x86_64.tgz" Run="/sbin/upgradepkg --install-new">
<URL>&repo;/packages/spl-0.6.5.1-unRAID6.1.3.x86_64.tgz</URL>
<MD5>24421f9d21a692435bb204e525aca0d5</MD5>
</FILE>

<FILE Name="&plugin;/packages/zfs-0.6.5.1-unRAID.6.1.3.x86_64.tgz" Run="/sbin/upgradepkg --install-new">
<URL>&repo;/packages/zfs-0.6.5.1-unRAID6.1.3.x86_64.tgz</URL>
<MD5>5c7b01a8d124b3460a773cb964d14355</MD5>
</FILE>

<FILE Run="/bin/bash">
<INLINE>
VERSION=$(uname -a)
if [[ $VERSION == *"4.1.7-unRAID"* ]];
then
  echo "Loading ZFS modules and importing pools (this could take some time....)"
  depmod 
  modprobe zfs 
  zpool import -a >> /dev/null
  echo "Finished"
  
elif [[ $VERSION == *"4.1.5-unRAID"* ]];
then
  echo "********************************************************************"
  echo "ZFS has been installed, but wont be loaded until you upgrade"
  echo "to unRAID version 6.1.3"
  echo "Please upgrade unRAID and reboot the server"
  echo ""
  echo "********************************************************************"
else
  echo ""
  echo "********************************************************************"
  echo "Unsupported kernel detected! This version of OpenZFS only works for"
  echo "unRAID version 6.1.3 - Older versions might be available on github"
  echo ""
  echo "********************************************************************"
  echo ""
fi
</INLINE>
</FILE>


